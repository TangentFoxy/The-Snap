{
  "manifest_version": 3,
  "name": "The Snap",
  "description": "Too many tabs open? Halve your workload.",
  "version": "1.1",
  "icons": {
    "16": "images/gauntlet16.png",
    "48": "images/gauntlet48.png",
    "128": "images/gauntlet128.png"
  },
  "author": "rose@tangentfox.com",
  "action": {},
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js",
    "scripts": [ "background.js" ],
    "persistent": true
  }
}
